{
  "workspaceDir": "/app",
  "stateDir": "/app/.ant",
  "gateway": {
    "enabled": true,
    "host": "0.0.0.0",
    "port": 5117
  },
  "ui": {
    "enabled": true,
    "host": "0.0.0.0",
    "port": 5117,
    "autoOpen": false,
    "staticDir": "ui/dist"
  },
  "logging": {
    "level": "info",
    "fileLevel": "debug",
    "filePath": "/app/.ant/logs/ant.log"
  },
  "providers": {
    "default": "ollama",
    "items": {
      "ollama": {
        "type": "ollama",
        "baseUrl": "http://ollama:11434",
        "model": "llama3.2:1b"
      },
      "openai": {
        "type": "openai",
        "baseUrl": "https://api.openai.com/v1",
        "apiKey": "",
        "model": "gpt-4o-mini"
      }
    },
    "fallbackChain": ["openai"]
  },
  "routing": {
    "chat": "ollama",
    "tools": "openai",
    "summary": "openai",
    "embeddings": "openai"
  },
  "whatsapp": {
    "sessionDir": "/app/.ant/whatsapp",
    "respondToGroups": false,
    "mentionOnly": true,
    "respondToSelfOnly": true,
    "allowSelfMessages": false,
    "resetOnLogout": true,
    "typingIndicator": true,
    "mentionKeywords": [],
    "ownerJids": [],
    "startupRecipients": []
  },
  "memory": {
    "enabled": true,
    "indexSessions": true,
    "sqlitePath": "/app/.ant/memory.sqlite",
    "embeddingsModel": "nomic-embed-text"
  },
  "scheduler": {
    "enabled": false
  },
  "mainAgent": {
    "enabled": false
  }
}

