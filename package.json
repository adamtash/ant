{
  "name": "ant",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "ant": "dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js start",
    "dev": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "cli": "tsx src/cli.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "ui:dev": "npm --prefix ui run dev",
    "ui:build": "npm --prefix ui run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@whiskeysockets/baileys": "^6.7.8",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "inquirer": "^9.2.0",
    "node-cron": "^4.2.1",
    "ora": "^8.0.1",
    "pino": "^9.6.0",
    "playwright": "^1.58.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.7.0",
    "ws": "^8.16.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.0.0"
  }
}
